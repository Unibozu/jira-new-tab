{
  "name": "JIRA - Open external links in a new tab",
  "version": "1.0",
  "description": "JIRA - Open external links in a new tab",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://*.atlassian.net/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}